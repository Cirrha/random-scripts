function submitToDiscord(message) {
  
  var formResponse = message.response;
  var itemResponses = formResponse.getItemResponses();
  var channelMsg;

  for (i in itemResponses){
    Logger.log("getItem().getTitle()=%s, getResponse()=%s", itemResponses[i].getItem().getTitle(), itemResponses[i].getResponse());
    
    var Name, channelMsg; 
   //For each question in the form, create an if statement like the one below
    if (itemResponses[i].getItem().getTitle() == "Name")
    {
      Name = itemResponses[i].getResponse();
    }
    
    //Formatting what Discord will output
    channelMsg = "\n\n ●・○・●・○・●・○・●・○・● \n**Name**: " + Name;
  }

  var discordUrl = 'DISCORD_WEBHOOK_URL';
  var payload = JSON.stringify({content: channelMsg });

  var params = {
    headers: {
      'Content-Type': 'application/json',
      'Accept': 'application/json'
    },
    method: "POST",
    payload: payload,
    muteHttpExceptions: true
  };

  var response = UrlFetchApp.fetch(discordUrl, params);
  
  
  if(response.getResponseCode() == "204.0" || response.getResponseCode() == "200" )
  {
    Logger.log("Seems ok"); 
  }
   else
  {
     MailApp.sendEmail({
    to: "YOUR_EMAIL",
    cc: "firestperson@gmail.com, secondperson@gmail.com",
    subject: "Form Error",
       htmlBody: "Hi! An error occured while trying to send a response tm your discord "  + "<br><br>" + "Error: " +response +  "<br><br>" + payload
  });
  }

  Logger.log(response.getContentText());
  Logger.log(payload);
  Logger.log(itemResponses);
  Logger.log(channelMsg);

}
